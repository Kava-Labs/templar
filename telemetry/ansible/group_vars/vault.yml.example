---
# Example Ansible Vault file - Replace these values with your secrets
# Use 'ansible-vault encrypt vault.yml' to encrypt this file

# Grafana credentials
grafana_security_admin_password: "CHANGE_ME"
grafana_security_secret_key: "CHANGE_ME"

# InfluxDB authentication token
grafana_influxdb_token: "CHANGE_ME_WITH_YOUR_INFLUXDB_TOKEN"

# Other secrets
# api_token: "CHANGE_ME"
# ssl_private_key_passphrase: "CHANGE_ME"

# Cloudflare R2 storage credentials (for Loki logs)
loki_r2_access_key_id: "CHANGE_ME_WITH_YOUR_R2_ACCESS_KEY_ID"
loki_r2_secret_access_key: "CHANGE_ME_WITH_YOUR_R2_SECRET_ACCESS_KEY"